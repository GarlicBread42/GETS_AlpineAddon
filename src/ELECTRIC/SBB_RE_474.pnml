						1 .. 2: return ITEM_SBB_RE_474_CARGO; 
						return CB_RESULT_NO_MORE_ARTICULATED_PARTS; }


item (FEAT_TRAINS , ITEM_SBB_RE_474_CARGO, 1684)
		{
	//property Block
			property 
			{
			//common properties
						name: 							string(STR_NAME_SBB_RE_474_CARGO);					
						climates_available: 			ALL_CLIMATES;
						introduction_date: 				date(2005,01,01);
						model_life: 					40;
						vehicle_life: 					40;
						retire_early: 					-10;							
						reliability_decay: 				20;
						cargo_allow_refit: 				[PASS] ;
						cargo_capacity: 				1 ;
						cost_factor: 					108;
						running_cost_base: 				RUNNING_COST_STEAM;						
						running_cost_factor: 			90;
						
			//Train specific properties
						sprite_id: 						SPRITE_ID_NEW_TRAIN;
						speed: 							140 km/h;
						refit_cost: 					0;
						track_type: 					ELEC;
						power:  						8704 hp;
						weight: 						87 ton;
						ai_engine_rank: 				0;
						engine_class: 					ENGINE_CLASS_ELECTRIC;
						visual_effect_and_powered: 		visual_effect_and_powered(VISUAL_EFFECT_ELECTRIC, 2, DISABLE_WAGON_POWER);
						dual_headed: 					0;
						tractive_effort_coefficient: 	0.345;
						length: 						8;
												
			}

		//Vehicels Graphics
			graphics
			{
			default							:switch_graphics_SBB_Re_474_Cargo;
			additional_text					:string(STR_CONCAT_3, 
											 string(STR_GAUGE_STANDARD), 
											 string(STR_TRACTION_ELECTRIC), 
											 string(STR_DESC_GOODS),);
			length							:switch_length_SBB_Re_474;
			articulated_part				:switch_articulated_SBB_Re_474_Cargo;
			purchase_cargo_capacity		    :0 ;
			cargo_capacity	                :return 0 ;
			purchase						:spriteset_SBB_Re_474_Cargo_purchase;
			cost_factor						:202;
			running_cost_factor				:switch_running_cost_SBB_Re_474;			
			visual_effect_and_powered		:switch_FX_SBB_Re_474;	
			}
	}	

					switch (FEAT_TRAINS, SELF, random_choose_SBB_Re_474_Special, switch_alternative_var_random_bits(1,1)) {
							1: spriteset_SBB_Re_474_Rastatt;
								spriteset_SBB_Re_474_Rastatt;}	

					switch(FEAT_TRAINS, SELF, switch_graphics_SBB_Re_474_Special, position_in_vehid_chain % 3) {
						0:	   spriteset_empty;
						1:     return random_choose_SBB_Re_474_Special;
						2:	   spriteset_empty;}

					switch (FEAT_TRAINS, SELF, switch_articulated_SBB_Re_474_Special, extra_callback_info1) {
						1 .. 2: return ITEM_SBB_RE_474_SPECIAL; 
						return CB_RESULT_NO_MORE_ARTICULATED_PARTS; }



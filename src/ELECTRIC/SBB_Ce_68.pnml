						switch(FEAT_TRAINS, SELF, switch_graphics_SBB_Ce_68_3_build, position_in_vehid_chain % 3) {
							0:	   spriteset_empty;
							1:     return switch_SBB_Ce_68_3_build_year;
							2:     spriteset_empty;}

						switch (FEAT_TRAINS, SELF, switch_articulated_SBB_Ce_68_3_build, extra_callback_info1) {
							1 .. 2: return ITEM_SBB_CE_68_3_BUILD; 
								return CB_RESULT_NO_MORE_ARTICULATED_PARTS; }



							    8: return visual_effect_and_powered(VISUAL_EFFECT_DISABLE, 0, DISABLE_WAGON_POWER);}
							
						switch (FEAT_TRAINS, PARENT, switch_FX_OBB_4746, vehicle_is_reversed) {
							0: switch_FX_OBB_4746_NONREV;
								switch_FX_OBB_4746_REV;}
								
						switch(FEAT_TRAINS,PARENT,switch_running_cost_OBB_4746, 1){
							return 2650;}
							
						switch (FEAT_TRAINS, PARENT, switch_reverse_OBB_4746_front, vehicle_is_reversed) {
							0: switch_loadstack_OBB_4746_front;
								switch_loadstack_OBB_4746_back_REV;}																

						switch (FEAT_TRAINS, PARENT, switch_reverse_OBB_4746_middle, vehicle_is_reversed) {
							0: switch_spritestack_OBB_4746_middle;
								switch_spritestack_OBB_4746_middle_REV;}
								
						switch (FEAT_TRAINS, PARENT, switch_reverse_OBB_4746_back, vehicle_is_reversed) {
							0: switch_loadstack_OBB_4746_back;
								switch_loadstack_OBB_4746_front_REV;}								

						switch(FEAT_TRAINS, SELF, switch_graphics_OBB_4746, position_in_vehid_chain % 9) {
							0:	   spriteset_empty;

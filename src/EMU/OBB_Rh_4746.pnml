							0: spriteset_OBB_Rh_4746_middle;
							1: spritegroup_OBB_Rh_4746_middle_loading;}	

						switch(FEAT_TRAINS, SELF, switch_spritestack_OBB_4746_middle_REV, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_OBB_Rh_4746_middle_REV;
							1: spritegroup_OBB_Rh_4746_middle_loading_REV;}

						switch(FEAT_TRAINS, SELF, switch_loadstack_OBB_4746_back, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 3 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {

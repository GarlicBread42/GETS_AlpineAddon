							1: spritegroup_OBB_Rh_4746_back_loading_REV;
							2: spriteset_OBB_Rh_4744_back_lights_front_REV;}
							
						switch(FEAT_TRAINS, SELF, switch_length_OBB_4746, position_in_vehid_chain % 9) {
							0: 4;
							1: 6;
							2: 4;
							3: 4;
							4: 6;
							5: 4;
							6: 4;
							7: 6;
							8: 4;}	
							
						switch(FEAT_TRAINS, SELF, switch_FX_OBB_4746_NONREV, position_in_vehid_chain % 9) {
								0: return visual_effect_and_powered(VISUAL_EFFECT_DISABLE,0, DISABLE_WAGON_POWER);
								1: return visual_effect_and_powered(VISUAL_EFFECT_DISABLE, 0, DISABLE_WAGON_POWER);
								2: return visual_effect_and_powered(VISUAL_EFFECT_ELECTRIC,-2, DISABLE_WAGON_POWER);	
					       3 .. 8: return visual_effect_and_powered(VISUAL_EFFECT_DISABLE, 0, DISABLE_WAGON_POWER);}							

						switch(FEAT_TRAINS, SELF, switch_FX_OBB_4746_REV, position_in_vehid_chain % 9) {
						   0 .. 5: return visual_effect_and_powered(VISUAL_EFFECT_DISABLE,0, DISABLE_WAGON_POWER);
								6: return visual_effect_and_powered(VISUAL_EFFECT_ELECTRIC,-2, DISABLE_WAGON_POWER);

								return visual_effect_and_powered(VISUAL_EFFECT_DIESEL, -1, DISABLE_WAGON_POWER);}	
							
						switch (FEAT_TRAINS, PARENT, switch_FX_SBB_Am_66, vehicle_is_reversed) {
							0: switch_FX_SBB_Am_66_1;
								switch_FX_SBB_Am_66_2;}													
						
						switch(FEAT_TRAINS,PARENT,switch_running_cost_SBB_Am_66, 1){
							return 1100;}
						
						switch (FEAT_TRAINS,SELF, switch_SBB_Am_66_build_year,build_year){
							  00 .. 1983: switch_reverse_SBB_Am_66_braun;
							1984 .. 2050: switch_reverse_SBB_Am_66_rot;
								switch_reverse_SBB_Am_66_rot;}												
						
					switch(FEAT_TRAINS, SELF, switch_graphics_SBB_Am_66_build, position_in_vehid_chain % 3) {

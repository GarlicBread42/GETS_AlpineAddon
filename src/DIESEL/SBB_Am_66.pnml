							0: 4;
							1: 3;
							2: 4;}
						
						switch(FEAT_TRAINS, SELF, switch_FX_SBB_Am_66_1, position_in_vehid_chain % 3) {
							0: return visual_effect_and_powered(VISUAL_EFFECT_DIESEL, -1, DISABLE_WAGON_POWER);
							1: return visual_effect_and_powered(VISUAL_EFFECT_DISABLE, 0, DISABLE_WAGON_POWER);

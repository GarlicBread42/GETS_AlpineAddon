						1:     switch_SBB_Am_66_build_year;
						2:	   spriteset_empty;}

					switch (FEAT_TRAINS, SELF, switch_articulated_SBB_Am_66_build, extra_callback_info1) {
						1: return ITEM_SBB_AM_66_BUILD; 
						2: return ITEM_SBB_AM_66_BUILD;
						return CB_RESULT_NO_MORE_ARTICULATED_PARTS; }									


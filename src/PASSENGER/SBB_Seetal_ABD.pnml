						switch(FEAT_TRAINS, SELF, switch_spritestack_SBB_Seetal_ABD_purchase, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_SBB_Seetal_ABD_purchase;
							1: spriteset_SBB_Seetal_ABD_window_purchase;}

						switch(FEAT_TRAINS, SELF, switch_loadstack_SBB_Seetal_ABD_window1, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 2 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {

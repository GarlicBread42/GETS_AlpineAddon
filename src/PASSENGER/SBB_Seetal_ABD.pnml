							1: spriteset_SBB_Seetal_ABD_window3;
							2: spritegroup_SBB_Seetal_ABD_loading;}
							
						switch(FEAT_TRAINS, SELF, switch_loadstack_SBB_Seetal_ABD_window4, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 2 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_SBB_Seetal_ABD;
							1: spriteset_SBB_Seetal_ABD_window4;
							2: spritegroup_SBB_Seetal_ABD_loading;}														

					random_switch(FEAT_TRAINS, SELF, random_loadstack_SBB_Seetal_ABD, bitmask(TRIGGER_VEHICLE_UNLOAD_ALL, TRIGGER_VEHICLE_NEW_LOAD,)) {
							1: switch_loadstack_SBB_Seetal_ABD_window1;
							1: switch_loadstack_SBB_Seetal_ABD_window2;
							1: switch_loadstack_SBB_Seetal_ABD_window3;
							3: switch_loadstack_SBB_Seetal_ABD_window4;}

						switch(FEAT_TRAINS, SELF, switch_length_SBB_Seetal_ABD, position_in_articulated_veh % 3) {
							0: 4;
							1: 5;
							2: 4;}	

					switch(FEAT_TRAINS,PARENT,switch_running_cost_SBB_Seetal_ABD, 1){
							return 950;}
							
						switch(FEAT_TRAINS, SELF, switch_cargo_capacity_SBB_Seetal_ABD, cargo_type_in_veh ) {
							PASS: 24;
							TOUR: 24;
						    MAIL: 24;
						    GOOD: 24;
						    VALU: 24;}							

						switch(FEAT_TRAINS, SELF, switch_graphics_SBB_Seetal_ABD, position_in_articulated_veh  % 3) {
							0:     spriteset_empty;
							1:     return random_loadstack_SBB_Seetal_ABD;
							2:     spriteset_empty;}

						switch (FEAT_TRAINS, SELF, switch_articulated_SBB_Seetal_ABD, extra_callback_info1) {
							1: return ITEM_SBB_SEETAL_ABD;
							2: return ITEM_MAIL_48;													
								return CB_RESULT_NO_MORE_ARTICULATED_PARTS;}							   												

																						

							0: spriteset_SBB_Eurofima_A_orange;
							1: spritegroup_SBB_Eurofima_loading_orange;}
							
						switch(FEAT_TRAINS, SELF, switch_loadstack_SBB_Eurofima_B_orange, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_SBB_Eurofima_B_orange;
							1: spritegroup_SBB_Eurofima_loading_orange;}	
							
						switch(FEAT_TRAINS, SELF, switch_loadstack_SBB_Eurofima_A_grau, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {

						switch(FEAT_TRAINS, SELF, switch_loadstack_SBB_Eurofima_A_IC, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_SBB_Eurofima_A_IC;
							1: spritegroup_SBB_Eurofima_loading_IC;}
							
						switch(FEAT_TRAINS, SELF, switch_loadstack_SBB_Eurofima_B_IC, [STORE_TEMP((getbits(extra_callback_info1, 8, 8) < 1 ? CB_FLAG_MORE_SPRITES : 0) | PALETTE_USE_DEFAULT, 0x100), getbits(extra_callback_info1, 8, 8)]) {
							0: spriteset_SBB_Eurofima_B_IC;
							1: spritegroup_SBB_Eurofima_loading_IC;}																										

						switch (FEAT_TRAINS, SELF, random_choose_SBB_Eurofima_orange, switch_alternative_var_random_bits(1, 1)) {
							1: switch_loadstack_SBB_Eurofima_A_orange;
							   switch_loadstack_SBB_Eurofima_B_orange;}
							   
						switch (FEAT_TRAINS, SELF, random_choose_SBB_Eurofima_grau, switch_alternative_var_random_bits(1, 1)) {
							1: switch_loadstack_SBB_Eurofima_A_grau;							
							   switch_loadstack_SBB_Eurofima_B_grau;}	
							   
						switch (FEAT_TRAINS, SELF, random_choose_SBB_Eurofima_IC, switch_alternative_var_random_bits(1, 1)) {
							1: switch_loadstack_SBB_Eurofima_A_IC;							
							   switch_loadstack_SBB_Eurofima_B_IC;}							   						   

					switch (FEAT_TRAINS,SELF,switch_build_year_SBB_Eurofima,build_year) {
							1900 .. 1992 : random_choose_SBB_Eurofima_orange;
							1993 .. 2000 : random_choose_SBB_Eurofima_grau;
							2001 .. 2100 : random_choose_SBB_Eurofima_IC;														
								random_choose_SBB_Eurofima_IC;}
						
						switch (FEAT_TRAINS,SELF,switch_current_year_SBB_Eurofima,current_year) {
							1900 .. 1992 : random_choose_SBB_Eurofima_orange;
							1993 .. 2000 : random_choose_SBB_Eurofima_grau;
							2001 .. 2100 : random_choose_SBB_Eurofima_IC;														
								random_choose_SBB_Eurofima_IC;}	   								
					
						switch(FEAT_TRAINS, SELF, switch_length_SBB_Eurofima, position_in_articulated_veh % 3) {
							0: 4;
							1: 7;
							2: 4;}	

						switch (FEAT_TRAINS, PARENT, switch_running_cost_SBB_Eurofima,  switch_check_Restaurant_0()) {
							1: return 0;
							0: return 2650;}	
							
						switch (FEAT_TRAINS, PARENT, switch_cargo_aging_SBB_Eurofima,  switch_check_Restaurant_0()) {
							1: return 233;
							0: return 185;}								
							
						switch(FEAT_TRAINS, SELF, switch_SBB_Eurofima_graphics_build, position_in_articulated_veh  % 3) {
							0:     spriteset_empty;
							1:     return switch_build_year_SBB_Eurofima;
							2:     spriteset_empty;}

						switch (FEAT_TRAINS, SELF, switch_articulated_SBB_Eurofima_build, extra_callback_info1) {
							1 .. 2: return ITEM_SBB_EUROFIMA_BUILD;												
								return CB_RESULT_NO_MORE_ARTICULATED_PARTS;}

